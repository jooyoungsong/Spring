package spring.di.ex2;

public class Myinfo {

	//생성자 주입으로 만들어 볼게요. 
	String name;
	int age;
	String addr;
	
	public Myinfo() {
		// TODO Auto-generated constructor stub
		
		this.name=name;
		this.age=age;
		this.addr=addr;
	}
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "Myinfo[name="+name+", age="+age+", addr="+addr+"]";
	}
}
